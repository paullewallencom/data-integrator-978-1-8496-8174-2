 <Field name="Txt" type="java.lang.String"><![CDATA[insert into	<%=odiRef.getTable("L","TARG_NAME","A")%> 
( 
	<%=odiRef.getColList("", "[COL_NAME]", ",\n\t", "", "INS and REW")%> 
) 

select  <%=odiRef.getColList("", "[COL_NAME]", ",\n\t", "", "INS and REW")%> from
(
select	<%=odiRef.getColList("", "[COL_NAME]", ",\n\t", "", "((INS and !TRG) and REW)")%>,
	<%=odiRef.getOption("PIVOT_FOR_CLAUSE")%> as PV_<%=odiRef.getOption("PIVOT_FOR_CLAUSE")%>, 
	<%=odiRef.getOption("PIVOT_AGGREGATE_COLUMN")%> as PV_<%=odiRef.getOption("PIVOT_AGGREGATE_COLUMN")%>
from	<%=odiRef.getFrom()%> 
where (1=1) <%=odiRef.getJoin()%>
<%=odiRef.getFilter()%>)
PIVOT(<%=odiRef.getOption("PIVOT_AGGREGATE_FUNCTION")%>  for  PV_<%=odiRef.getOption("PIVOT_FOR_CLAUSE")%> in
(<%=odiRef.getColList("", "[EXPRESSION] as [COL_NAME]", ",\n\t", "", "((INS and TRG) and REW)")%>))]]></Field>
</Object>
